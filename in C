#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
    int n;
    scanf("%d",&n);
    long int sum=0;
    long long int a[n];
     for(int i=0;i<n;i++)
        scanf("%lld\t",&a[i]);
    for(int i=0;i<n;i++)
    {
        scanf("%lld\t",&a[i]);
        sum=sum+a[i];
    }
    printf("%lld",sum);
    return 0;

}
